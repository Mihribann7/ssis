          <ProjectItem
            Name="ScriptMain.cs"
            Encoding="UTF8"><![CDATA[#region Help:  Introduction to the script task

#endregion


#region Namespaces
using Microsoft.SqlServer.Dts.Runtime;
using System;
using System.Data;
using System.Data.SqlClient;
using System.Net.Mail;
using System.Windows.Forms;
using System.Net;
#endregion

namespace ST_62523488030b4786bef06d6be317545d
{
    /// <summary>
    /// ScriptMain is the entry point class of the script.  Do not change the name, attributes,
    /// or parent of this class.
    /// </summary>
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {

        public void Main()
        {
            string body = "";
            string connStr = "Data Source=WIN-IQDREHRC5DK;Initial Catalog=ssis14;Integrated Security=SSPI;";
            string query = "SELECT CustomerID, CustomerName FROM Customers_ErrorTable";

            using (SqlConnection conn = new SqlConnection(connStr))
            {
                conn.Open();
                using (SqlCommand cmd = new SqlCommand(query, conn))
                {
                    using (SqlDataReader reader = cmd.ExecuteReader())
                    {
                        while (reader.Read())
                        {
                            body += $"CustomerID: {reader[0]}, Name: {reader[1]}\n";
                        }
                    }
                }
            }

            if (!string.IsNullOrEmpty(body))
            {
                MailMessage mail = new MailMessage("nosirovamehribon7@gmail.com", "m.nosirova@newuu.uz");
                mail.Subject = "Missing Customers Detected in SSIS Package";
                mail.Body = body;

                SmtpClient smtp = new SmtpClient("smtp.gmail.com");
                smtp.Port = 587;
                smtp.Credentials = new System.Net.NetworkCredential("nosirovamehribon7@gmail.com", "your_app_password");
                smtp.EnableSsl = true;

                smtp.Send(mail);
            }

            Dts.TaskResult = (int)ScriptResults.Success;
        }

        #region ScriptResults declaration
    
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

    }
}]]></ProjectItem>
