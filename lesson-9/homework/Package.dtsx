         * */
        #endregion


        /// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
        {
            string folderPath = Dts.Variables["User::FolderPath"].Value.ToString();
            string filePattern = Dts.Variables["User::FileName"].Value.ToString();
            bool fileFound = false;
            string fullPath = "";

            try
            {
                // Check if the folder exists
                if (!Directory.Exists(folderPath))
                {
                    // Log folder not found error
                    Dts.Events.FireError(0, "Folder Check", "❌ Folder does not exist: " + folderPath, "", 0);
                }
                else
                {
                    // Look for files matching pattern
                    string[] files = Directory.GetFiles(folderPath, filePattern);
                    if (files.Length > 0)
                    {
                        foreach (string file in files)
                        {
                            FileInfo fi = new FileInfo(file);
                            if (fi.Length > 0)
                            {
                                fullPath = file;
                                fileFound = true;
                                break;
                            }
                        }

                        if (!fileFound)
                        {
                            // Log empty file issue
                            Dts.Events.FireError(0, "File Validation", "❌ Matching file(s) found but all are empty!", "", 0);
                        }
                    }
                    else
                    {
                        // Log no file match
                        Dts.Events.FireError(0, "File Search", "❌ No file found with pattern: " + filePattern, "", 0);
                    }
                }

                // Set the variables
                Dts.Variables["User::FileExists"].Value = fileFound;
                Dts.Variables["User::FullFilePath"].Value = fullPath;

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "Script Task Error", "❌ Unexpected error: " + ex.Message, "", 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

